{
    "apiVersion": 2,
    "name": "description-list-block/description-list",
    "title": "Description List Block",
    "category": "text",
    "icon": "clipboard",
    "supports": {
        "html": true,
        "anchor": true,
        "typography": {
            "fontSize": false
        },
        "__unstablePasteTextInline": true
    },
    "attributes": {
        "termsFontSize": {},
        "descriptionsFontSize": {},
        "termsColor": {},
        "descriptionsColor": {},
        "termsMargin": {},
        "descriptionsMargin": {},
        "indent": {},
        "spacing": {},
        "style":{},
        "list": {
            "type": "array",
            "default": [],
            "source": "query",
            "selector": "dt,dd",
            "query": {
                "content": {
                    "type": "string",
                    "source": "html"
                },
                "tag": {
                    "type": "string",
                    "default": "dt",
                    "source": "tag"
                }
            }
        }
    },
    "textdomain": "description-list-block",
    "editorScript": "file:./build/index.js",
    "editorStyle": "file:./build/index.css",
    "style": "file:./build/style-index.css"
}
